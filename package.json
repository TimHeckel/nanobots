{
  "name": "nanobots",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 6100",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cli:dev": "bun run cli/index.ts",
    "cli:build": "bun build cli/index.ts --compile --outfile dist/nanobots",
    "cli:build:linux": "bun build cli/index.ts --compile --target=bun-linux-x64 --outfile dist/nanobots-linux-x64",
    "cli:build:mac-arm": "bun build cli/index.ts --compile --target=bun-darwin-arm64 --outfile dist/nanobots-darwin-arm64",
    "cli:build:mac-x64": "bun build cli/index.ts --compile --target=bun-darwin-x64 --outfile dist/nanobots-darwin-x64",
    "cli:build:all": "bun run cli:build:linux && bun run cli:build:mac-arm && bun run cli:build:mac-x64",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "e2e": "vitest run --config vitest.config.e2e.ts",
    "e2e:ci": "vitest run --config vitest.config.e2e.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:agents": "vitest run --config vitest.config.agents.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.29",
    "@ai-sdk/react": "^3.0.88",
    "@neondatabase/serverless": "^1.0.2",
    "@octokit/auth-app": "^8.2.0",
    "@octokit/rest": "^22.0.1",
    "ai": "^6.0.86",
    "jose": "^6.1.3",
    "next": "16.1.6",
    "picomatch": "^4.0.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^4.0.18",
    "agent-browser": "^0.10.0",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
